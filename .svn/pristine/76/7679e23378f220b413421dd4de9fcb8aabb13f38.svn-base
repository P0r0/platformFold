<config>
 
    <params>
        <param name="DOISLANDSCAPE" value="true" required="1" showName="doIsLandScape" desc="是否横屏显示登录界面" bWriteInManifest="0" bWriteInClient="1" />
        <param name="DOISSUPPORTOFFLINE" value="false" required="1" showName="doIsSupportOffline" desc="是否支持离线模式，默认值为false" bWriteInManifest="0" bWriteInClient="1" />
        <param name="DOISFIXED" value="false" required="1" showName="doIsFixed" desc="是否为定额支付" bWriteInManifest="0" bWriteInClient="1" />
        <param name="SETAPPNAME" value="360游戏" required="1" showName="setAppName" desc="" bWriteInManifest="0" bWriteInClient="1" />
 		<param name="PRODUCTNAME" value="productName" required="1" showName="productName" desc="" bWriteInManifest="0" bWriteInClient="1" />
        <param name="APPUSERNAME" value="appUserName" required="1" showName="appUserName" desc="" bWriteInManifest="0" bWriteInClient="1" />
        
        <param name="GAME_ID" value="id" required="1" showName="id" desc="_id 当前情景， 支持 enterServer（登录） ， levelUp（升级） ， createRole（创建角色）" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_ROLEID" value="roleId" required="1" showName="roleId" desc="roleId 当前登录的玩家角色 ID，若无，可传入 userid" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_ROLENAME" value="roleName" required="1" showName="roleName" desc="roleName 当前登录的玩家角色名，不能为空，不能为 null，若无，传入“游戏名称+username”， 如“皇室战争大名鼎鼎”" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_ROLELEVEL" value="roleLevel" required="1" showName="roleLevel" desc="roleLevel 当前登录的玩家角色等级，必须为数字，且不能为 0，若无，传入 1" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_ZONEID" value="zoneId" required="1" showName="zoneId" desc="zoneId 当前登录的游戏区服 ID，必须为数字，且不能为 0，若无，传入 1" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_ZONENAME" value="zoneName" required="1" showName="zoneName" desc="zoneName 当前登录的游戏区服名称，不能为空，不能为 null，若无，传入游戏名称+” 1 区”，如“梦幻西游 1 区”" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_BALANCE" value="balance" required="1" showName="balance" desc="balance 当前用户游戏币余额，必须为数字，若无，传入 0" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_VIP" value="vip" required="1" showName="vip" desc="vip 当前用户 VIP 等级，必须为数字，若无，传入 1" bWriteInManifest="0" bWriteInClient="1" />
        <param name="GAME_PARTYNAME" value="partyName" required="1" showName="partyName" desc="partyName 当前用户所属帮派，不能为空，不能为 null，若无，传入”无帮派”" bWriteInManifest="0" bWriteInClient="1" />
        
        <param name="QHOPENSDK_APPKEY" value="08158bf9f09b919790a63f10c381be52" required="1" showName="" desc="请在360应用开发平台注册申请" bWriteInManifest="1" bWriteInClient="1" />
        <param name="QHOPENSDK_PRIVATEKEY" value="dd09f2d209a268973d55a29d7f7961a4" required="1" showName="" desc="此处 value 为 PRIVATEKEY 不是 APPSECRET，而是md5(app_secret +”#”+ app_key)" bWriteInManifest="1" bWriteInClient="1" />
        <param name="QHOPENSDK_APPID" value="201696301" required="1" showName="" desc="请在360应用开发平台注册申请" bWriteInManifest="1" bWriteInClient="1" />
        <param name="QHOPENSDK_WEIXIN_APPID" value="wx02faa6a503e262e5" required="1" showName="" desc="从微信开放平台申请的appid，游戏需要去申请自己的appid" bWriteInManifest="1" bWriteInClient="1" />
       
        <param name="DC_APPKEY" value="02522a2b2726fb0a03bb19f2d8d9524d" required="1" showName="" desc="" bWriteInManifest="1" bWriteInClient="1" />
    	
    </params>
 
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="copyRes" from="root" to="" />
    </operations>
 
    <plugins>
        <plugin name="com.u8.sdk._360User" type="1" desc="用户登录接口"/>
 		<plugin name="com.u8.sdk._360Pay" type="2" desc="用户支付接口" />
 		<plugin name="com.u8.sdk._360Share" type="4" desc="用户分享接口" />
    </plugins>
    
    <version>
        <name>360</name>
        <versionCode>1</versionCode>
        <versionName>1.3.6</versionName>
    </version>
 
</config>