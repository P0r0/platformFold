<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params> 
        <param name="APP_ID" required="1" showName="appID" desc="" bWriteInManifest="0" bWriteInClient="1" />
        <param name="CPKEY" required="1" showName="CPKEY" desc="" bWriteInManifest="0" bWriteInClient="1" />
        <param name="CPID" required="1" showName="CPID" desc="" bWriteInManifest="0" bWriteInClient="1" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.VivoUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.VivoPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>VIVO</name>
        <versionCode>1</versionCode>
        <versionName>2.0.2</versionName>
    </version>    
    
</config>