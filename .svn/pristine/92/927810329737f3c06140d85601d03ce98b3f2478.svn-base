<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <params> 
        <param name="wostore_billing_otherpay" value = "true" required="0" showName="unicomPayAble" desc="true|false" bWriteInManifest="1" bWriteInClient="0" />
        <param name="wostore_billing_chinaunicom" value = "true" required="0" showName="unicomPayAble" desc="true|false" bWriteInManifest="1" bWriteInClient="0" />
        <param name="wostore_billing_chinamobile" value = "true" required="0" showName="mobilePayAble" desc="true|false" bWriteInManifest="1" bWriteInClient="0" />
        <param name="wostore_billing_chinatele" value = "true" required="0" showName="telePayAble" desc="true|false" bWriteInManifest="1" bWriteInClient="0" />        
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="copyRes" from="runtime" to="runtime"/>
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.UnicomUser" type="1" desc="用户登录接口"/>
        <plugin name="com.u8.sdk.UnicomPay" type="2" desc="用户支付接口"/>
    </plugins>
    
    <version>
        <name>UNICOM</name>
        <versionCode>1</versionCode>
        <versionName>4.0.2</versionName>
    </version>    
    
</config>